{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/thuytienpham17/FIT3179/main/Data_Vis2/dataset2023.csv"
  },
  "title": {
    "text": "Happiness Score Range in Regions",
    "fontSize": 20,
    "offset": 20
  },
  "width": 450,
  "height": 350,
  "config": {
    "background": "#FFF3E2",
    "view": { "stroke": "transparent" },
    "axis": {
      "grid": false,
      "labels": false,
      "ticks": false
    },
    "axisX": {"titleFontSize": 13}, 
      "axisY": {"titleFontSize": 13, "labels": true},
      "legend": {
        "titleFontSize": 13, 
        "labelFontSize": 12.5
      }
  },
  "mark": "boxplot",
  "encoding": {
    "x": {
      "field": "Regional Indicator",
      "type": "ordinal",
      "title": null
    },
    "y": { "field": "Ladder Score", "type": "quantitative", "title": "Happiness Score"},
    "color": {
      "field": "Regional Indicator"
    }
  }
}
